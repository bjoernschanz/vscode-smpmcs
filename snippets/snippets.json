{
    "Assign": {
        "prefix": ["++ASSIGN", "++ ASSIGN"],
        "body": [
            "++ASSING   SOURCEID(${1:sourceid})",
            "           TO(${2:sysmodid})",
            ".",
            "${0}"
        ]
    },

    "Delete": {
        "prefix": ["++DELETE", "++ DELETE"],
        "body": [
            "++DELETE(${1:name}) SYSLIB(${2:ALL})",
            "          /* ALIAS(${3:name}) */",
            ".",
            "${0}"
        ]
    },
    "Function": {
        "prefix": ["++FUNCTION", "++ FUNCTION"],
        "body": [
            "++FUNCTION(${1:sysmodid}) REWORK(${2:yyyyddd})",
                "\tDESC(${3:expr})",
            ".",
            "++VER(${4:srel}) FMID(${5:fmid})",
                "\t/* DELETE(${6:sysmodid}) */",
                "\t/* PRE(${6:sysmodid}) */",
                "\t/* SUP(${7:sysmodid}) */",
                "\t/* REQ(${8:sysmodid}) */",
            ".",
            "${0}"
        ]
    },

    "Usermod": {
        "prefix": ["++USERMOD", "++ USERMOD"],
        "body": [
            "++USERMOD(${1:sysmodid}) REWORK(${2:yyyyddd})",
                "\tDESC(${3:expr})",
            ".",
            "++VER(${4:srel}) FMID(${5:fmid})",
                "\t/* PRE(${6:sysmodid}) */",
                "\t/* SUP(${7:sysmodid}) */",
                "\t/* REQ(${8:sysmodid}) */",
            ".",
            "${0}"
        ]
    },


    "HFS Element": {
        "prefix": ["++HFS", "++ HFS"],
        "body": [
            "++HFS(${1:expr}) SYSLIB(${2:expr}) DISTLIB(${3:expr})",
                "\t${0}",
            "."
        ]
    }
}