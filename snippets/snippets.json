{
    "Usermod": {
        "prefix": ["++USERMOD", "++ USERMOD"],
        "body": [
            "++USERMOD(${1:sysmodid}) REWORK(${2:yyyyddd})",
                "\tDESC(${3:expr})",
            ".",
            "++VER(${4:ver}) FMID(${5:fmid})",
                "\t/* PRE(${6:sysmodid}) */",
                "\t/* SUP(${7:sysmodid}) */",
                "\t/* REQ(${8:sysmodid}) */",
            ".",
            "${0}"
        ]
    },


    "HFS Element": {
        "prefix": ["++HFS", "++ HFS"],
        "body": [
            "++HFS(${1:expr}) SYSLIB(${2:expr}) DISTLIB(${3:expr})",
                "\t${0}",
            "."
        ]
    }
}